    ae.find('a[ui-sref="ecfs.search.filings(dateObj.params)"]').prepend('<span class="ae-compliance-indent" style="display:none;">Browse filings by </span>');

    // Apply tooltip text as aria-labels
    ae.find('[tooltip]').each(function() {
      ae.find(this).attr('aria-label',ae.find(this).attr('tooltip'));
      ae.find(this).append('<span class="ae-text-reader"> '+ae.find(this).attr('tooltip')+' </span>');
    });

    // Give inputs lacking a type the type of "text" for styling purposes
    ae.find('input:not([type])').attr('type','text');

    // Give extra detail text to link
    ae.find('.fa-file-text').after('<span class="ae-compliance-indent" style="display:none;">Filing Detail </span>');

    // Make non-form look like a plain div in RDM and not be announced as a form
    ae.find('div[ui-view="filingview"] form.form-horizontal').attr('data-ae-blurbtype','div');

    // Make upload drop box act like an anchor
    ae.find('.drop-box').attr('data-ae-blurbtype','button');

    // Make .btn class elements act like buttons
    setTimeout(function(){
      ae.find('.btn').attr('data-ae-blurbtype','button');
    }, 500);

    // Append filing number to links for clarity
    ae.find('table.proceedings a:contains(New Filing), table.proceedings a:contains(Express Reply)').each( function() {
      ae.find(this).append('<span class="ae-compliance-indent" style="display:none;"> for ' + ae.find(this).closest('tr').find('a:first').text() + '</span>');
    });

    // Prepend proceeding to filing numbers for clarity
    ae.find('.proceedings.table tr').each(function() {
      ae.find(this).find('a:first').prepend('<span class="ae-compliance-indent" style="display:none;">Proceeding </span>');
    });

    ae.find('.pagination-page a').prepend('<span class="ae-compliance-indent" style="display:none;">Page </span>');
    ae.find('.pagination-page a:contains(...) .ae-compliance-indent').text('Additional Pages');


	// Apply accessible PDF changes only to ECFS pages
    if (String(window.location.pathname).indexOf("/ecfs/") == 0) {

      // Exclude anchors in a nav menu from having "accessible PDF added
      ae.find('.nav a[href*=".pdf"]').addClass('ae-exclude-pdf');

      // Create PDF links that point to us
      ae.find('a[href*=".pdf"]:not(.ae-accessible-link):not(.ae-exclude-pdf)').each( function() {
        if (ae.find(this).next('.ae-accessible-link').length < 1) {
          var href = ae.find(this).attr('href');
          if(href.substring(0,1) === '/') {
            href = window.location.origin + href;
          } else if(href.substring(0,4) !== 'http') {
            href = ae.find(this).get(0).href;
          }
          var href = encodeURIComponent(href);
          var url = 'http://wsv3.audioeye.com/convertpdf.php?url=' + href;
          ae.find(this).after('<a class="ae-accessible-link" rel="nofollow" href="'+url+'" onclick="return confirm(\'Are you sure you want to proceed to an accessible version of the document?\')"><img class="ae-accessible-pdf" src="//wsv3cdn.audioeye.com/images/AudioEye_Tools_PDFAccessibility_icon.svg" alt="An accessible version of this document may exist, click here to access that version" /></a>');
        }
      });
    }

    ae.find('#menu-primary').removeAttr('aria-hidden');


    // Fix Date Range Labels
    ae.find('label[for="dateReceivedStart"] span,label[for="dateReceivedEnd"] span,label[for="datePostedStart"] span,label[for="datePostedEnd"] span,label[for="dateCommentStart"] span,label[for="dateCommentEnd"] span,label[for="dateReplyCommentStart"] span,label[for="dateReplyCommentEnd"] span,label[for="dateCreatedStart"] span,label[for="dateCreatedEnd"] span,label[for="dateClosedStart"] span,label[for="dateClosedEnd"] span').css('visibility','').addClass('ae-compliance-indent');

    ae.find('label[for="dateReceivedStart"],label[for="dateReceivedEnd"]').prepend('<span class="ae-compliance-indent" style="display:none;">Date Received Range. </span> ');
    ae.find('label[for="datePostedStart"],label[for="datePostedEnd"]').prepend('<span class="ae-compliance-indent" style="display:none;">Posted Date Range. </span> ');
    ae.find('label[for="dateCommentStart"],label[for="dateCommentEnd"]').prepend('<span class="ae-compliance-indent" style="display:none;">Comment Date. </span> ');
    ae.find('label[for="dateReplyCommentStart"],label[for="dateReplyCommentEnd"]').prepend('<span class="ae-compliance-indent" style="display:none;">Reply Comment Date. </span> ');
    ae.find('label[for="dateCreatedStart"],label[for="dateCreatedEnd"]').prepend('<span class="ae-compliance-indent" style="display:none;">Date Created Range. </span> ');
    ae.find('label[for="dateClosedStart"],label[for="dateClosedEnd"]').prepend('<span class="ae-compliance-indent" style="display:none;">Date Closed Range. </span> ');

    // Remove unnecessary role from selects that causes issues with JAWS
    ae.find('select[role]').removeAttr('role');

    // Label nav menus
    ae.find('#search-subnav .menu-secondary').attr('aria-label','Filings and Proceedings Search');
    ae.find('#filing-subnav .menu-secondary').attr('aria-label','Submit a Filing');
    ae.find('#status-subnav .menu-secondary').attr('aria-label','Check Filing Status');
