<script type="text/javascript">
	!function(r){void 0!==window.AudioEye?window.AudioEye.manualRemediations.push(r):r($ae("html"))}(function(ele){
	
        //vague text - use nearby heading to append to Learn more and Read more
        ele.find('a').filter(function(){
            return this.innerHTML.toLowerCase().trim() == 'learn more' || this.innerHTML.toLowerCase().trim() == 'read more';
    	}).each(function(){ 
          	var ae_mel_newText = '';
			if($ae(this).prev('h6, h5, h4, h3, h2, h1').length != 0){
            	ae_mel_newText = $ae(this).prev('h6, h5, h4, h3, h2, h1').text();
            } else if($ae(this).prevAll('h6, h5, h4, h3, h2, h1').length != 0){
              	ae_mel_newText = $ae(this).prevAll('h6, h5, h4, h3, h2, h1').first().text();
            }
			var $parents = $ae(this).parent();
			var parent = $parents;
			while (ae_mel_newText == '' && parent.nodeName != 'body') {
   				ae_mel_newText = $parents.find('h6, h5, h4, h3, h2, h1').last().text();
				$parents= $parents.parent();
			}
            if(ae_mel_newText != '') {
        		$ae(this).append(' <span class="ae-compliance-indent ae-reader-visible" style="display:none;"> about ' + ae_mel_newText + ' </span> ');
            }
        });
      
      	//Redundant link title
      	ele.outerFind('a[title]').each(function(){
      		var ae_mel_title = $ae(this).attr('title').toLowerCase().trim();
      		var ae_mel_hipertext = $ae(this).text().toLowerCase().trim();
          	if(ae_mel_hipertext == '' && $ae(this).find('img[alt]').length > 0){
                ae_mel_hipertext = $ae(this).find('img[alt]').attr('alt').toLowerCase().trim();
            }
            if(ae_mel_hipertext.indexOf(ae_mel_title) > -1){
                $ae(this).removeAttr('title');
            }
        });
      
      	//empty link
      	ele.outerFind('a[href="#"]').contents().filter(function(){ 
			return this.nodeType == 8 && this.nodeType != 3 && this.nodeType != 1;
		}).parent().attr({'tabindex':'-1', 'aria-hidden':'true'});
      
      	//Vague link hipertext - click here
      	ele.outerFind('a[href]').filter(function(){
    		return this.innerHTML.toLowerCase().trim() == 'click here';
		}).each(function(){ 	
          	var $ae_mel_vaguelink = $ae(this);
			var ae_mel_index = $ae_mel_vaguelink.parent().contents().index($ae(this));
			if(ae_mel_index < $ae_mel_vaguelink.parent().contents().length) {
				//get phrase after click here
				var ae_mel_next = $ae_mel_vaguelink.parent().contents().eq(ae_mel_index +1).text();					
				//var ae_mel_text = ae_mel_next.substring(0, ae_mel_next.indexOf('.'));
				var ae_mel_text = ae_mel_next;
				//if not too long and not too short, use the rest of the phrase
              	if(ae_mel_text.length < 141 && ae_mel_text.length > 4 ){
                  	$ae_mel_vaguelink.append(' <span class="ae-compliance-indent ae-reader-visible" style="display: none;"> ' + ae_mel_text + ' </span> ');
					var ae_mel_regex = new RegExp(ae_mel_text + "(?![\\s\\S]*" + ae_mel_text + ")");
       				$ae_mel_vaguelink.parent().html(function(_,h){
                      	return h.replace(ae_mel_regex, '<span aria-hidden="true" role="presentation">' + ae_mel_text + '</span>');
                    });                	
				}              
   			}
        });
      
	});
  

