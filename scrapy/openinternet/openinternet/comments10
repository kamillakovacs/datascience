    //alt update
    ae.find('img[src="https://prototype.fcc.gov/sites/default/files/styles/media_thumbnail/public/mo-leadership-thumbnail.jpg?itok=1NP8k2Q0"]').attr('alt', 'Thumbnail picture of Commissioner Michae.findl O\'Rielly');
