	!function(r){void 0!==window.AudioEye?window.AudioEye.manualRemediations.push(r):r($ae("html"))}(function(ele){
		ele.outerFind('.rtecenter iframe').attr('title','Tutorials');                                    
      	ele.find('iframe').not('[title]').each(function(){
            if(!this.hasAttribute('src')){
              $ae(this).attr({'title':'empty frame', 'role': 'presentation', 'aria-hidden': 'true'});     
            } else {
              if($ae(this).attr('src') == 'about:blank'){
                  $ae(this).attr({'title':'blank frame', 'role': 'presentation', 'aria-hidden': 'true'});   
              }
              if($ae(this).attr('src').indexOf('transition.fcc.gov') > -1){
