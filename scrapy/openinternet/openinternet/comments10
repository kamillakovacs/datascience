    ae.outerFind('[datepicker-popup-wrap] button').attr('tabindex','0');
    //ae.outerFind('[datepicker-popup-wrap]').removeAttr('tabindex').attr('role','presentation').attr('aria-hidden','true');
    //ae.find('table.filings.table').attr('data-ae-blurbtype','');
  });

</script>            
